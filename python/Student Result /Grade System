name = input("Enter student name: ")
marks = []

for i in range(5):
    mark = int(input(f"Enter mark {i+1}: "))
    marks.append(mark)

total = sum(marks)
percentage = total / 5

if percentage >= 80:
    grade = "A"
elif percentage >= 60:
    grade = "B"
elif percentage >= 40:
    grade = "C"
else:
    grade = "Fail"

print("\nStudent Name:", name)
print("Total Marks:", total)
print("Percentage:", percentage)
print("Grade:", grade)
