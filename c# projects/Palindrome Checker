using System;

class Palindrome
{
    static void Main()
    {
        Console.Write("Enter word: ");
        string s = Console.ReadLine();
        string rev = "";

        for (int i = s.Length - 1; i >= 0; i--)
            rev += s[i];

        Console.WriteLine(s == rev ? "Palindrome" : "Not Palindrome");
    }
}
