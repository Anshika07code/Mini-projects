#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    int guess, number;

    srand(time(0));
    number = rand() % 100 + 1;

    printf("Guess the number (1 to 100)\n");

    do {
        printf("Enter guess: ");
        scanf("%d", &guess);

        if (guess > number)
            printf("Too high!\n");
        else if (guess < number)
            printf("Too low!\n");
        else
            printf("ðŸŽ‰ Correct Guess!\n");

    } while (guess != number);

    return 0;
}
